{
    "name": "assurdeal/nova-action-button-selectors",
    "description": "A Laravel Nova asset.",
    "keywords": ["laravel", "nova", "buttons", "action", "selectors"],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/assurdeal/nova-action-button-selectors",
    "authors": [
        {
            "name": "Sascha Brendel",
            "email": "mail@lednerb.de"
        },
        {
            "name": "Pitchayakit Pahamak",
            "email": "pitchayakit.p@gmail.com",
            "role": "Software Engineer"
        },
        {
            "name": "Percy Mamedy",
            "email": "percy.mamedy@assurdeal.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Assurdeal\\ActionButtonSelector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Assurdeal\\ActionButtonSelector\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
